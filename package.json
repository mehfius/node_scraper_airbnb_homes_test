{
  "name": "node_scraper_homes_test",
  "version": "1.0.0",
  "description": "Atualizar informacoes do host do Airbnb",
  "main": "index.js",
  "scripts": {
    "save_jobs": "node src/jobs/jobs_crawler_save_html.js",
    "save_rooms": "node src/rooms/rooms_crawler_save_html.js",    
    "clear_jobs": "node src/jobs/jobs_crawler_optimize.js",
    "clear_rooms": "node src/rooms/rooms_crawler_optimize_html.js",    
